alpha=alpha,
lfcThreshold=lfcThresh,
altHypothesis='greaterAbs',
pAdjustMethod='BH')
summary(res)
message(glue("No. of significant DE genes (FDR < {alpha}): "), sum(res$padj < alpha, na.rm=TRUE))
